{
  "name": "@link-to-code/editor",
  "version": "1.0.0",
  "description": "link-to-code editor package",
  "scripts": {
    "start": "start-storybook -p 6007",
    "build": "rm -rf ./lib && tsc --build --clean && tsc && pnpm build:assets",
    "build:assets": "copyfiles -u 1 src/**/*.module.css src/assets/* lib",
    "build:watch": "pnpm run build"
  },
  "files": [
    "./lib",
    "./config",
    "./README.md"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./config/tailwind": {
      "default": "./config/tailwind.js"
    },
    "./config/daisyui": {
      "default": "./config/daisyui.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "typescript": "~4.8.4",
    "eslint": "~8.26.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-react": "~7.31.10",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "storybook": "~6.5.13",
    "@storybook/addon-actions": "~6.5.13",
    "@storybook/addon-docs": "~6.5.13",
    "@storybook/addon-essentials": "~6.5.13",
    "@storybook/addon-interactions": "~6.5.13",
    "@storybook/addon-links": "~6.5.13",
    "@storybook/react": "~6.5.13",
    "@storybook/testing-library": "~0.0.13",
    "@types/react": "~18.0.24",
    "copyfiles": "~2.4.1",
    "@types/react-dom": "~18.0.8",
    "autoprefixer": "~10.4.13",
    "postcss": "~8.4.18",
    "@storybook/addon-postcss": "~2.0.0",
    "tailwindcss": "~3.2.1",
    "daisyui": "~2.36.1",
    "@tailwindcss/typography": "~0.5.7",
    "@typescript-eslint/eslint-plugin": "~5.41.0",
    "@typescript-eslint/parser": "~5.41.0",
    "eslint-plugin-import": "~2.26.0",
    "@svgr/webpack": "~5.4.0",
    "url-loader": "~4.1.1",
    "storybook-css-modules": "~1.0.8"
  },
  "peerDependencies": {
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "autoprefixer": "~10.4.13",
    "postcss": "~8.4.18",
    "tailwindcss": "~3.2.1",
    "@tailwindcss/typography": "~0.5.7",
    "daisyui": "~2.36.1"
  },
  "dependencies": {
    "clsx": "~1.2.1",
    "@heroicons/react": "~2.0.12",
    "@radix-ui/react-dialog": "~1.0.2",
    "@radix-ui/react-checkbox": "~1.0.1",
    "@codesandbox/sandpack-react": "~1.17.0",
    "react-tabs": "~5.1.0",
    "@monaco-editor/react": "~4.4.6"
  }
}
