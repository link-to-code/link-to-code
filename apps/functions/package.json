{
  "name": "@link-to-code/functions",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc --clean",
    "build:watch": "tsc",
    "emulator:start": "firebase emulators:start --only functions,firestore,storage",
    "serve": "concurrently \"npm:build:watch\" \"npm:emulator:start\"",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0",
    "@link-to-code/domain": "workspace:*",
    "zod": "~3.19.1",
    "cors": "~2.8.5",
    "express": "~4.18.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^4.5.4",
    "@firebase/app-types": "~0.7.0",
    "eslint-config-prettier": "~8.5.0",
    "@link-to-code/types": "workspace:*",
    "concurrently": "~7.4.0",
    "@types/express": "~4.17.14",
    "@types/cors": "~2.8.12"
  },
  "private": true
}
